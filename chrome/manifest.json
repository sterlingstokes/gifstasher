{
  "manifest_version": 2,
  "name": "gifstasher",
  "description": "Store your reaction gifs for quick and easy access!",
  "version": "1.0",
  "permissions": [
    "storage",
    "contextMenus",
    "clipboardRead",
    "clipboardWrite"
  ],
  "icons": {
    "16": "img/icon-bitty.png",
    "48": "img/icon-small.png",
    "128": "img/icon-large.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
    "background": {
    "scripts": [
      "js/contextMenu.js"
    ]
  }
}